
LOSS_MAXIMO = 0.12
LATENCIA_MAXIMA = 500

loss_atual = float(input("digite o loss atual: "))
latencia_em_ms_atual = int(input("digite a latência em ms: "))
if loss_atual > LOSS_MAXIMO or latencia_em_ms_atual > LATENCIA_MAXIMA:
	print("❌O sistema não está dentro dos parâmetros aceitáveis.")
else: 
    print("✅O sistema está dentro dos parâmetros aceitáveis.")
